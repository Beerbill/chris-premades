      },
      {
        "id": "babonus",
        "type": "module",
        "compatibility": {
          "minimum": "12.1.2",
          "verified": "12.1.2"
        }
      },
      {
        "id": "socketlib",
        "type": "module",
        "compatibility": {
          "minimum": "1.1.0",
          "verified": "1.1.0"
