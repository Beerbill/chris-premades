  "packFolders": [
    {
      "name": "Chris's Premades",
      "sorting": "a",
      "color": "#348f2d",
      "packs": [
        "CPRSpells",
        "CPRSpellFeatures",
        "CPRSummonFeatures",
        "CPRSummons"
      ]
    }
  ],
  "changelog": "https://github.com/chrisk123999/chris-premades/blob/master/CHANGELOG.md",
